{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "name": "Movie",
    "description": "A motion picture."
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "director": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::person.person"
    },
    "actor": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person.person"
    },
    "genre": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "datePublished": {
      "type": "date"
    },
    "productionCompany": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization"
    },
    "trailer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::videoobject.videoobject"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images"]
    },
    "contentRating": {
      "type": "string"
    },
    "sameAs": {
      "type": "string"
    },
    "aggregateRating": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::aggregaterating.aggregaterating"
    },
    "review": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    }
  }
}
