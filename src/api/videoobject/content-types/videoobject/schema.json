{
  "kind": "collectionType",
  "collectionName": "videoobjects",
  "info": {
    "name": "Video Object",
    "description": "A video file."
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "uploadDate": {
      "type": "date"
    },
    "duration": {
      "type": "string"
    },
    "embedUrl": {
      "type": "string"
    }
  }
}
